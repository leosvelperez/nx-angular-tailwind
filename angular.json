          },
          "configurations": {
            "production": {
              "devServerTarget": "app2:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/app2-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["app2"]
    },
    "lib1": {
      "projectType": "library",
      "root": "libs/lib1",
      "sourceRoot": "libs/lib1/src",
      "prefix": "my-org",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/lib1"],
          "options": {
            "jestConfig": "libs/lib1/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/lib1/src/**/*.ts",
              "libs/lib1/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "lib2": {
      "projectType": "library",
      "root": "libs/lib2",
      "sourceRoot": "libs/lib2/src",
      "prefix": "my-org",
      "architect": {
        "build": {
          "builder": "./dist/libs/angular:ng-packagr-lite",
          "outputs": ["dist/libs/lib2"],
          "options": {
            "project": "libs/lib2/ng-package.json",
            "tailwindConfig": "libs/tailwind/tailwind.config.js"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/lib2/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/lib2/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/lib2"],
          "options": {
            "jestConfig": "libs/lib2/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/lib2/src/**/*.ts",
              "libs/lib2/src/**/*.html"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["angular", "tailwind"]
    },
    "lib3": {
      "projectType": "library",
      "root": "libs/lib3",
      "sourceRoot": "libs/lib3/src",
      "prefix": "my-org",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/lib3"],
          "options": {
            "project": "libs/lib3/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/lib3/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/lib3/tsconfig.lib.json"
            }
          },
