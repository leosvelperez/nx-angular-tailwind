              "libs/lib3/src/**/*.ts",
              "libs/lib3/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "tailwind": {
      "root": "libs/tailwind",
      "sourceRoot": "libs/tailwind",
      "projectType": "library",
      "architect": {},
      "tags": []
    }
  }
}
